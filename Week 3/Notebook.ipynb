    "except OSError:\n",
    "    print(\"OS error occurred\")\n",
    "    sys.exit(1)\n",
    "else:\n",
    "    with log:\n",
    "        ports = list(log)\n",
    "        portCount = {}\n",
    "\n",
    "        for port in ports:\n",
    "            port = int(port.strip())\n",
    "            if int(port) < 1024:\n",
    "                if port in portCount:\n",
