    "import os\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "curPath = os.path.abspath(\"Notebook.ipynb\")\n",
    "logPath = os.path.join(os.path.dirname(curPath), \"ports.log\")\n",
    "\n",
    "try:\n",
    "    log = open(logPath)\n",
    "except FileNotFoundError:\n",
